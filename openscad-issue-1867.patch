new file mode 100644
index 00000000..7e503ec3
--- /dev/null
+++ b/testdata/scad/3D/issues/issue1867.scad
@@ -0,0 +1,25 @@
+polyhedron(points = [
+[78.1831, 62.349, 10],
+[97.4928, -22.2521, -5],
+[-97.4928, -22.2521, 5],
+[-78.1831, 62.349, -10],
+[78.1831, 62.349, 200],
+[97.4928, -22.2521, 200],
+[-97.4928, -22.2521, 200],
+[-78.1831, 62.349, 200]
+],
+faces = [
+[0, 1, 4],
+[1, 2, 5],
+[2, 3, 6],
+[3, 0, 7],
+[5, 4, 1],
+[6, 5, 2],
+[7, 6, 3],
+[4, 7, 0],
+[4, 5, 6],
+[4, 6, 7],
+
+[3, 2, 1, 0],
+
+]);
